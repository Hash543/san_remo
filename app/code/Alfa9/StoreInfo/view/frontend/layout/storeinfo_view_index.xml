    <body>
        <referenceBlock name="page.main.title" remove="true" />
        <attribute name="class" value="storeinfo"/>
        <referenceContainer name="content">
            <block class="Alfa9\StoreInfo\Block\Stockists" name="stockists.stores.individual" template="Alfa9_StoreInfo::individualStore.phtml"/>
        </referenceContainer>
    </body>
</page>
