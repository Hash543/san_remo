<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<body>
		<referenceContainer name="content">
			<block class="PSS\WordPress\Block\Term\View" name="wp.postlist.wrapper" template="post/list/wrapper.phtml">
				<block class="PSS\WordPress\Block\Post\ListPost" name="wp.post.list" template="post/list.phtml">
